<% if honeybadger_js_enabled? %>
  <%= javascript_include_tag "https://js.honeybadger.io/v6.11/honeybadger.min.js" %>
  <meta name="honeybadger-api-key" content="<%= Honeybadger.config[:api_key] %>">
  <meta name="honeybadger-environment" content="<%= Rails.env %>">
  <meta name="honeybadger-revision" content="<%= Honeybadger.config[:revision] %>">
  <meta name="honeybadger-user-context" content="<%= honeybadger_user_context.to_json %>">
<% end %>
